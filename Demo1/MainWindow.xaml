<Window x:Class="Demo1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Process" Command="{Binding ProcessCommand}" HorizontalAlignment="Left" Margin="10,218,0,0" VerticalAlignment="Top" Width="497" Height="30"/>
        <Button Content="Cancel"  HorizontalAlignment="Left" Margin="10,253,0,0" VerticalAlignment="Top" Width="497" Height="30"/>
        <ListBox ItemsSource="{Binding Output}" HorizontalAlignment="Left" Height="167" Margin="10,46,0,0" VerticalAlignment="Top" Width="497"/>

    </Grid>
</Window>
